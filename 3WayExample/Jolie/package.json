{
  "name": "3wayexample",
  "version": "1.0.0",
  "description": "Can be used to run an example of 3 services implementing the inbox/outbox pattern",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "start": "concurrently --handle-input --default-input-target 3 \"jolie ServiceA/serviceA.ol\" \"jolie ServiceB/serviceB.ol\" \"jolie ServiceC/serviceC.ol\" \"sleep 8 &&./reader.sh\""
  },
  "author": "Mathias Jensen",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
